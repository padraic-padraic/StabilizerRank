number = {7505},
pages = {351--5},
pmid = {24919152},
publisher = {Nature Publishing Group},
title = {{Contextuality supplies the 'magic' for quantum computation}},
url = {http://www.nature.com/doifinder/10.1038/nature13460$\backslash$nhttp://www.ncbi.nlm.nih.gov/pubmed/24919152},
volume = {510},
