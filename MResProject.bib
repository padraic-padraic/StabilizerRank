issn = {1050-2947},
journal = {Physical Review A},
month = {aug},
number = {2},
pages = {022312},
title = {{Qudit color codes and gauge color codes in all spatial dimensions}},
url = {http://arxiv.org/abs/1503.08800 http://dx.doi.org/10.1103/PhysRevA.92.022312 http://link.aps.org/doi/10.1103/PhysRevA.92.022312},
volume = {92},
