title = {{Classical simulation of quantum computation, the Gottesman-Knill theorem, and slightly beyond}},
url = {http://arxiv.org/abs/0811.0898v2$\backslash$nhttp://arxiv.org/abs/0811.0898},
volume = {quant-ph},
year = {2008}
}
