@article{Gottesman2009,
abstract = {Quantum states are very delicate, so it is likely some sort of quantum error$\backslash$ncorrection will be necessary to build reliable quantum computers. The theory of$\backslash$nquantum error-correcting codes has some close ties to and some striking$\backslash$ndifferences from the theory of classical error-correcting codes. Many quantum$\backslash$ncodes can be described in terms of the stabilizer of the codewords. The$\backslash$nstabilizer is a finite Abelian group, and allows a straightforward$\backslash$ncharacterization of the error-correcting properties of the code. The stabilizer$\backslash$nformalism for quantum codes also illustrates the relationships to classical$\backslash$ncoding theory, particularly classical codes over GF(4), the finite field with$\backslash$nfour elements. To build a quantum computer which behaves correctly in the$\backslash$npresence of errors, we also need a theory of fault-tolerant quantum$\backslash$ncomputation, instructing us how to perform quantum gates on qubits which are$\backslash$nencoded in a quantum error-correcting code. The threshold theorem states that$\backslash$nit is possible to create a quantum computer to perform an arbitrary quantum$\backslash$ncomputation provided the error rate per physical gate or time step is below$\backslash$nsome constant threshold value.},
archivePrefix = {arXiv},
arxivId = {0904.2557v1},
author = {Gottesman, Daniel},
eprint = {0904.2557v1},
file = {:Users/padraic/Documents/UCL/MRes/Quantum Communication {\&} Computation/Case Study/Papers/0904.2557v1.pdf:pdf},
journal = {Proceedings of Symposia in Applied Mathematics},
pages = {1--46},
title = {{An Introduction to Quantum Error Correction and Fault-Tolerant Quantum  Computation}},
volume = {0000},
year = {2009}
}
