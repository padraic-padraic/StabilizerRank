journal = {Quantum Inf. Comput.},
number = {3-4},
pages = {263},
title = {{The LU-LC conjecture, diagonal local operations and quadratic forms over GF(2)}},
url = {http://arxiv.org/abs/0707.4000},
volume = {8},
year = {2007}
}
